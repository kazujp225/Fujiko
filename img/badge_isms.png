        .acd-pc{
            display: none;
        }
        .acd-sp{
            display: block;
        }
    
        .acd-wrapper {
            flex-direction: column;
        }
        .acd-contents {
            display: flex;
            align-items: center;
        }
        .acd-case {
            width: 100%;
        }
        .acd-arrow {
            position: absolute;
            right: 8px;
            border-left: 10px solid transparent;
            border-right: 10px solid transparent;
            border-top: 10px solid #F95D84;
        }
        .cb6accordion li:nth-of-type(5) {
            line-height: 45px;
        }
        .cb6accordion li img {
            width: 100%!important;
            max-width: 100%;
